# API Key Usage Inspector Environment Variables

# QuickNode API Configuration
# Get your API key from QuickNode Console
QUICKNODE_ENABLED=true
QUICKNODE_CONSOLE_APIKEY=your_quicknode_api_key_here

# Birdeye API Configuration
# Your Birdeye account credentials
BIRDEYE_ENABLED=true
BIRDEYE_EMAIL=your_email@example.com
BIRDEYE_PASSWORD=your_password_here

# Debug Configuration
# Enable debug mode for additional logging
DEBUG_ENABLED=false

# Prometheus Push Gateway Configuration
# URL and job name for pushing metrics
PUSH_GATEWAY_ENABLED=false
PUSH_GATEWAY_URL=http://localhost:9091
PUSH_GATEWAY_JOB=cron-apikey-usage

# FlareSolverr Configuration (for bypassing Cloudflare)
# Enable FlareSolverr to bypass Cloudflare protection
FLARESOLVER_ENABLED=false
FLARESOLVER_ENDPOINT=http://localhost:8191/v1
FLARESOLVER_PROXY=http://your-proxy:port

# DNS Resolution Configuration
# Custom DNS mapping is handled in code, but you can override if needed
# DNS_MAP={"multichain-api.birdeye.so": "37.59.30.17"}

# Docker Configuration (for build.py script)
# DockerHub credentials for building and pushing images
DOCKERHUB_USERNAME=your_dockerhub_username
DOCKERHUB_TOKEN=your_dockerhub_access_token

# CoinMarketCap API Configuration
# Your CoinMarketCap account credentials for usage tracking
CMC_ENABLED=true
CMC_EMAIL=your_cmc_email@example.com
CMC_PASSWORD=your_cmc_password_here
CMC_SESSION_TOKEN='["",""]'

TWITTERAPI_ENABLED='true'
TWITTERAPI_APIKEY=your_twitter_api_key_here
# TWITTERAPI_APIKEY='["your_twitter_api_key_here","apikey2","apikey3"]'
